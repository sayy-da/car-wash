      <script>
        const heroSection = document.getElementById('heroSection');
        const mainTitle = document.getElementById('mainTitle');
        const subtitle = document.getElementById('subtitle');

        const slides = [
          {
            image: 'https://images.unsplash.com/photo-1575844611398-2a68400b437c?fm=jpg&q=60&w=3000',
            title: 'Best Water Service Center',
            subtitle: 'Polishing, Washing, Vacuum Cleaning',
            style: 'style-1'
          },
          {
            image: 'https://39678065.fs1.hubspotusercontent-na1.net/hub/39678065/hubfs/Washing%20cars.jpg?width=1920&height=845&name=Washing%20cars.jpg',
            title: 'Premium Car Care Services',
            subtitle: 'Professional cleaning & detailing solutions',
            style: 'style-2'
          }
        ];

        let currentSlide = 0;

        function updateSlide() {
          const slide = slides[currentSlide];

          // Update background image
          heroSection.style.backgroundImage = `linear-gradient(rgba(0, 0, 0, 0.2) 0%, rgba(0, 0, 0, 0.6) 100%), url("${slide.image}")`;

          // Remove all style classes
          heroSection.classList.remove('style-1', 'style-2');

          // Add new style class
          heroSection.classList.add(slide.style);

          // Update text content
          mainTitle.textContent = slide.title;
          subtitle.textContent = slide.subtitle;

          // Move to next slide
          currentSlide = (currentSlide + 1) % slides.length;
        }

        // Initialize first slide
        updateSlide();

        // Change slides every 4 seconds (2 seconds zoom + 2 seconds display)
        setInterval(updateSlide, 4000);
      </script>

